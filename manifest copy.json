{
  "manifest_version": 2,
  "name": "Thurston's Note Taking Extension",
  "version": "0.9",

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.4.1.min.js", "content.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png"  
  },

  "background": {
  "scripts": ["background.js"]
  }

}